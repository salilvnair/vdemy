{
  "name": "vdemy",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "electron": "./node_modules/.bin/electron .",
    "pack-m": "build -m",
    "pack-m-debug": "env DEBUG=electron-builder build -m",
    "pack-w": "build -w",
    "pack-w-debug": "env DEBUG=electron-builder build -w",
    "pack-w64": "build -w --x64",
    "pack-w64-debug": "env DEBUG=electron-builder build -w --x64"
  },
  "build": {
    "asar": false,
    "appId": "com.vdemy",
    "productName": "Vdemy",
    "files": [
      "**/*",
      "build"
    ]
  },
  "private": true,
  "dependencies": {
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.38.2"
  }
}
